{
    "name": "sudippalash/blade-components",
    "description": "Laravel package for provides a set of blade components",
    "keywords": [
        "Sudip",
        "Laravel provides a set of blade components",
        "Provides a set of blade components",
        "Laravel"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sudippalash/blade-components",
    "authors": [
        {
            "name": "Sudip Palash",
            "email": "palash.sudip@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
          "Sudip\\BladeComponents\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sudip\\BladeComponents\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
